<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HSNP.Mobile.Views.WebMISPage"
             Title="Web MIS">
<VerticalStackLayout VerticalOptions="Fill">
       <HorizontalStackLayout HorizontalOptions="Center">
			 <Image Source="hsnp_logo" Margin="0,0,20,0" HorizontalOptions="Center"  HeightRequest="100" />
              <Image Source="ndma_logo" HorizontalOptions="Center" HeightRequest="90" />
			 <BoxView HeightRequest="1" BackgroundColor="Silver"></BoxView>
		</HorizontalStackLayout>
           
            <Button Margin="0,50" Text="Go to Web MIS" HorizontalOptions="CenterAndExpand" BackgroundColor="Black" TextColor="White" Clicked="LaunchWebClicked"></Button>
        <Label HorizontalTextAlignment="Center" Text="{Binding AppVersion}" VerticalOptions="EndAndExpand"></Label>
    </VerticalStackLayout>
</ContentPage>